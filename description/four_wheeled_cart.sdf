        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="front_right_wheel_joint" type="fixed">
      <parent>chassis</parent>
      <child>front_right_wheel</child>
      <pose>-0.25 -0.15 0</pose>
    </joint>
  </model>
</sdf>
